{
  "name": "flipboxdigital/relay-transform",
  "description": "Transform Middleware for Relay",
  "version": "1.0.0",
  "keywords": [
    "relay",
    "flipbox",
    "transform",
    "middleware",
    "psr-7"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Flipbox Factory",
      "email": "hello@flipboxfactory.com"
    }
  ],
  "require": {
    "php": ">=7.0",
    "flipboxdigital/transform": "^1.0",
    "flipboxdigital/relay": "^2.0",
    "flipboxdigital/http-stream": "^2.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^5.0",
    "squizlabs/php_codesniffer": "^2.0"
  },
  "autoload": {
    "psr-4": {
      "Flipbox\\Relay\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Flipbox\\Relay\\Tests\\": "tests/src/"
    }
  },
  "config": {
    "optimize-autoloader": true
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://packagist.org"
    }
  ]
}